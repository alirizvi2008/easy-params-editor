{
    "manifest_version": 2,
    "name": "Easy Params Editor",
    "description": "Chrome extension to edit url params",
    "version": "1.0",
    "icons": {
        "128": "icons/params-icon.png",
        "48": "icons/params-icon.png",
        "16": "icons/params-icon.png"
    },
    "browser_action": {
        "default_icon": "icons/params-icon.png",
        "default_title": "Easy Params Editor",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ]
}